export wd=$(dirs +0)

windres -I "$WXWIDGETS_ROOT\\include" resource.rc -o resource.o && g++ -Wl,-subsystem,windows -mwindows -Os -MD -MP -o "./bin/${wd##*/}" --static *.cpp resource.o -lwxmsw31u_core -lwxbase31u -lwxbase31u_net -lwxbase31u_xml -lwxexpat -lwxjpeg -lwxmsw31u_adv -lwxmsw31u_aui -lwxmsw31u_gl -lwxmsw31u_html -lwxmsw31u_media -lwxmsw31u_propgrid -lwxmsw31u_ribbon -lwxmsw31u_richtext -lwxmsw31u_stc -lwxmsw31u_webview -lwxmsw31u_xrc -lwxpng -lwxregexu -lwxscintilla -lwxtiff -lwxzlib -lkernel32 -luser32 -lgdi32 -lcomdlg32 -lwinspool -lwinmm -lshell32 -lcomctl32 -lole32 -loleaut32 -luuid -lrpcrt4 -ladvapi32 -lwsock32 -lwininet -lversion -lshlwapi -luxtheme -loleacc -DBOOST_CONFIG_SUPPRESS_OUTDATED_MESSAGE -lboost_system-mt -lboost_filesystem-mt -ltbb -D__WXMSW__ -D_UNICODE -DUNICODE -DNDEBUG && strip --strip-all "./bin/${wd##*/}.exe"
