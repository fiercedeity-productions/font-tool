export RTLIBCFG=static

EXTRA=

if [ $DEVENV == "x64" ]; then
EXTRA=""
else
EXTRA="/SAFESEH"
fi


rc resource.rc && cl /EHsc /O2 /Ot /MT /MP /Fe:./bin/main.exe /fp:fast *.cpp resource.res tbb.lib wxmsw31u_core.lib wxbase31u.lib wxbase31u_net.lib wxbase31u_xml.lib wxexpat.lib wxjpeg.lib wxmsw31u_adv.lib wxmsw31u_aui.lib wxmsw31u_gl.lib wxmsw31u_html.lib wxmsw31u_media.lib wxmsw31u_propgrid.lib wxmsw31u_ribbon.lib wxmsw31u_richtext.lib wxmsw31u_stc.lib wxmsw31u_webview.lib wxmsw31u_xrc.lib wxpng.lib wxregexu.lib wxscintilla.lib wxtiff.lib wxzlib.lib kernel32.lib user32.lib gdi32.lib comdlg32.lib winspool.lib winmm.lib shell32.lib comctl32.lib ole32.lib oleaut32.lib uuid.lib rpcrt4.lib advapi32.lib libcurl_a.lib /DCURL_STATICLIB /DBOOST_CONFIG_SUPPRESS_OUTDATED_MESSAGE $1 /DNDEBUG /D_NDEBUG_ /link $EXTRA /NXCOMPAT /DYNAMICBASE  && rm *.obj
